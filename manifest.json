{
    "name": "Amazon",
     "content_scripts": [
     {
      "css": ["content.css"],
       "matches": ["https://www.google.com/*"

     ],
      
       "js": [ "content.js","jquery.js"]
     }
   ],
     "permissions": [
            "storage","tabs",
            "https://www.google.com/*"
          ],
    "background" : {
      "scripts" : [
        "jquery.js", "background.js"]
    },
    "version": "1.0",
     
    "browser_action": {
        "default_title": "Set products",
        "default_icon": "checkbox.png"
    },
    "icons": {
        "128": "checkbox.png",
        "48": "checkbox.png",
        "16": "checkbox.png"
      },
    
    "manifest_version": 2
  }