{
    "name": "Amazon",
     "content_scripts": [
     {
      "run_at":"document_end",
      "css": ["content.css"],
       "matches": ["*://*.google.com/*"],
       "js": ["jquery.js", "content.js"]
     }
   ],
     "permissions": [
            "storage","tabs",
            "*://*.google.com/*"
          ],
    "background" : {
      "scripts" : [
        "jquery.js", "background.js"]
    },
    "version": "1.0",
     
    "browser_action": {
        "default_title": "Set products",
        "default_icon": "checkbox.png"
    },
    "icons": {
        "128": "checkbox.png",
        "48": "checkbox.png",
        "16": "checkbox.png"
      },
    
    "manifest_version": 2
  }